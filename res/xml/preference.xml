<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">


    <!-- Emergency alert settings for ETWS. -->
    <PreferenceCategory 
        android:title="微信"
        android:key="category_wechat">

        <CheckBoxPreference 
            android:defaultValue="false"
            android:key="enable_wechat"
            android:summary="更改微信聊天字体,添加快捷方式等"
            android:title="开启微信" />

    </PreferenceCategory>

    <PreferenceCategory 
        android:title="微博"
        android:key="category_weibo">

        <CheckBoxPreference 
            android:defaultValue="false"
            android:key="enable_wechat"
            android:summary="去除广告等"
            android:title="开启微博" />

    </PreferenceCategory>
    
    <PreferenceCategory 
        android:title="流量监控"
        android:key="category_netmgr">
        
        <CheckBoxPreference
            android:defaultValue="false"
            android:key="enable_netmgr"
            android:title="开启流量监控" />
    </PreferenceCategory>
   
    <PreferenceCategory 
        android:title="超级调试"
        android:key="category_superdebug">
        
        <CheckBoxPreference
            android:defaultValue="false"
            android:key="enable_superdebug"
            android:title="开启超级调试" />
    </PreferenceCategory>
    
</PreferenceScreen>
